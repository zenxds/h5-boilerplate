{
  "name": "h5-boilerplate",
  "version": "0.1.0",
  "description": "h5-boilerplate",
  "main": "build/main.js",
  "scripts": {
    "start": "BABEL_ENV=development webpack-dev-server -w --open --env=development",
    "build": "BABEL_ENV=production webpack --env=production",
    "prettier": "prettier src/**/*.js --write",
    "deploy": "npm run build && scp ./build/* ubuntu@domain:/home/ubuntu/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn prettier",
      "git add"
    ]
  },
  "author": "",
  "publishConfig": {},
  "repository": {
    "type": "git",
    "url": ""
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "dx-mock": "^0.1.0",
    "eslint": "^5.5.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "image-webpack-loader": "^4.3.1",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "moment": "^2.22.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-px2rem": "^0.3.0",
    "prettier": "^1.15.3",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.2",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7",
    "webpack-html-one": "^0.3.2"
  },
  "dependencies": {
    "dx-mlib": "^0.3.0",
    "fastclick": "^1.0.6",
    "normalize.css": "^8.0.0"
  }
}
